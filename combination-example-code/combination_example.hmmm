-- 
-- Register conventions:
--   r13 = return value
--   r14 = return address
--   r15 = stack pointer
--   r0-r12 = general purpose

-- Main program
    -- Read n from user
    READ r1             -- r1 = n
    
    -- Read k from user
    READ r2             -- r2 = k
    
